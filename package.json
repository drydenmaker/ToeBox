{
  "destinationDir": "dist/",
  "name": "toebox",
  "description": "A WordPress theme built on the most popular front-end framework for developing responsive, mobile first projects on the web.",
  "version": "0.1.1",
  "keywords": [
    "css",
    "less",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "homepage": "http://gettoebox.com",
  "author": "drydenmaker",
  "scripts": {
    "test": "grunt test"
  },
  "style": "dist/css/toebox.css",
  "less": "less/toebox.less",
  "main": "./dist/js/npm",
  "repository": {
    "type": "git",
    "url": "https://github.com/drydenmaker/toebox.git"
  },
  "bugs": {
    "url": "https://github.com/drydenmaker/toebox/issues"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/drydenmaker/toebox/blob/master/LICENSE"
  },
  "devDependencies": {
    "btoa": "~1.1.2",
    "glob": "~4.3.5",
    "grunt": "~0.4.5",
    "grunt-autoprefixer": "~2.2.0",
    "grunt-banner": "~0.3.1",
    "grunt-contrib-clean": "~0.6.0",
    "grunt-contrib-compress": "~0.13.0",
    "grunt-contrib-concat": "~0.5.0",
    "grunt-contrib-connect": "~0.9.0",
    "grunt-contrib-copy": "~0.7.0",
    "grunt-contrib-csslint": "~0.4.0",
    "grunt-contrib-cssmin": "~0.11.0",
    "grunt-contrib-jade": "~0.14.0",
    "grunt-contrib-jshint": "~0.11.0",
    "grunt-contrib-less": "~1.0.0",
    "grunt-contrib-qunit": "~0.5.2",
    "grunt-contrib-uglify": "~0.7.0",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-csscomb": "~3.0.0",
    "grunt-exec": "~0.4.6",
    "grunt-html": "~2.0.0",
    "grunt-jekyll": "~0.4.2",
    "grunt-jscs": "~1.2.0",
    "grunt-saucelabs": "~8.5.0",
    "grunt-sed": "~0.1.1",
    "load-grunt-tasks": "~3.1.0",
    "markdown-it": "^3.0.4",
    "npm-shrinkwrap": "^200.0.0",
    "time-grunt": "~1.0.0"
  },
  "engines": {
    "node": "~0.10.1"
  },
  "jspm": {
    "main": "js/toebox",
    "directories": {
      "example": "examples",
      "lib": "dist"
    },
    "shim": {
      "js/toebox": {
        "imports": "jquery",
        "exports": "$"
      }
    },
    "buildConfig": {
      "uglify": true
    }
  }
}
